{
  "name": "banxway-backend",
  "version": "1.0.0",
  "description": "Backend API server for Banxway freight forwarding communication hub",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "migrate": "node migrate-all.js",
    "migrate:prod": "echo 'Set DATABASE_URL env var and run: node migrate-all.js'",
    "seed": "ts-node scripts/seed.ts",
    "worker:email-poller": "node dist/workers/email-poller.worker.js",
    "worker:email-poller:dev": "ts-node src/workers/email-poller.worker.ts",
    "worker:whatsapp": "node dist/workers/whatsapp-processor.worker.js",
    "worker:whatsapp:dev": "ts-node src/workers/whatsapp-processor.worker.ts",
    "worker:transcription": "node dist/workers/transcription.worker.js",
    "worker:transcription:dev": "ts-node src/workers/transcription.worker.ts",
    "workers:all": "concurrently \"npm run worker:email-poller\" \"npm run worker:whatsapp\" \"npm run worker:transcription\"",
    "workers:all:dev": "concurrently \"npm run worker:email-poller:dev\" \"npm run worker:whatsapp:dev\" \"npm run worker:transcription:dev\"",
    "seed:workflows": "ts-node src/scripts/seed-workflows.ts"
  },
  "keywords": [
    "freight",
    "forwarding",
    "communications",
    "api"
  ],
  "author": "Banxway",
  "license": "ISC",
  "dependencies": {
    "@huggingface/inference": "^2.6.4",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "imap": "^0.8.19",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.6.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.17.2",
    "prom-client": "^15.1.0",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/imap": "^0.8.43",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mailparser": "^3.4.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}