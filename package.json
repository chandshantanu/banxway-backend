    "bullmq": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "imap": "^0.8.19",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.6.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
